<div class="col-xs-12 col-lg-3 col-md-3 place-teaser">
    <a href="{{ path('place', {'slug': place.slug,'id': place.id}) }}">
        <div>
            {{ dump(place.images) }}
            {% for image in place.images %}
                {{ dump(image) }}
            {% endfor %}
            {% if place.images %}
                {%  set image = place.images | first %}
                {#<img src={{image.imageUrl }}>#}
            {% endif %}
            <div class="place-description">
                <h3>{{ place.name }}</h3>
                <p>{{ place.address ~ ' ' ~ place.postalCode ~ ' ' ~ place.city}}</p>
            </div>
        </div>
    </a>
</div>